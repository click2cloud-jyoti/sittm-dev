<body class=""  style="overflow: auto; outline: none; height: 575px;" id="managerViewBody">
    <div ng-app="ManagerViewApp" ng-controller="ManagerViewCntrl" ng-cloak>

        <div class="page-container" ng-init="init('<%= JSON.stringify(data) %>')">

            <div class="row ">
                <div class="col-md-8">
                    <div class="main-head">
                        <a href="" onclick="window.history.back()" ng-if="managerLevel!='level2'"><img src="images/icons/back_icon.svg" class="mr-2 back-icon">Manager Info
                        </a>
                    </div>

                </div>
                <div class="col-md-4">
                    <div class="updates float-right text-right">
                        <!-- <p class="mb-0">Last Updated: <span>4/23/2020 4:13 PM</span></p> -->
                        <p class="mb-0">Last Login: <span>{{lastLogin || null}}</span></p>
                    </div>
                </div>
            </div>
            <hr>
        </div>
        <div class="page-container">
            <div class="row">
                <div class="col-md-4">
                    <div class="manager-block "  style="overflow: auto; outline: none;">
                        <div class="widget-content p-0 border-box">
                            <div class="widget-content-wrapper mb-2">
                                <div class="widget-content-left mr-3">
                                    <img width="90" class="rounded-circle" src="images/placeholder_image.svg" alt="" style="width: 70%;">
                                </div>
                                <div class="widget-content-left">
                                    <div class="widget-heading mb-2">{{managerDetails.full_name}}({{managerDetails.alias}})</div>
                                    <div class="widget-para">
                                        <p></p>
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="profile-table">
