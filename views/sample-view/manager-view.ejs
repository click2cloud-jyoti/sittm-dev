                                <table class="align-middle mb-0 table table-borderless mng_card">

                                    <tbody>
                                        <tr class="border-box">
                                            <td class="text-muted">Standard Title</td>
                                            <td>: {{managerDetails.title}}</td>
                                        </tr>
                                        <tr class="border-box">
                                            <td class="text-muted">Phone</td>
                                            <td>: {{managerDetails.phone_no}}</td>
                                        </tr>
                                        <tr class="border-box">
                                            <td class="text-muted">IC Level</td>
                                            <td></td>
                                        </tr>
                                        <!-- <tr class="border-box">
                                            <td class="text-muted">Months In Role</td>
                                            <td>:{{managerDetails.months_in_role}}</td>
                                        </tr>
                                        <tr class="border-box">
                                            <td class="text-muted">Months In Microsoft</tdtd>
                                            <td>:{{managerDetails.months_in_microsoft}}</td>
                                        </tr> -->
                                        <tr class="border-box">
                                            <td class="text-muted">Global Manager</td>
                                            <td>: {{managerDetails.global_manager}}</td>
                                        </tr>
                                        <tr class="border-box">
                                            <td class="text-muted">Company Country</td>
                                            <td>: {{managerDetails.company_country}}</td>
                                        </tr>
                                        <tr class="border-box">
                                            <td class="text-muted">Time zone
                                            <td>: {{managerDetails.timezone}}</td>

                                        </tr>
                                        <tr class="border-box">
                                            <td class="text-muted">Area</td>
                                            <td>: {{managerDetails.based_geo}}</td>
                                        </tr>
                                        <tr>
                                            <td class="text-muted">Diverse</td>
                                            <td>: {{managerDetails.diverse}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-8 pl-0">
                    <div class="team-table">
                        <div class="main-card  card">
                            <div class="row">
                                <div class="col-md-6">
                                    <h3 class="card-head float-left">Team</h3>
                                </div>
                                <div class="col-md-6">
                                    <button class="btn fliter-button float-right" data-toggle="modal"
                                        data-target="#timeZoneModal">
                                        <p>Filter</p><img src="images/filter_icon.svg" class="float-right">
                                    </button>
                                </div>
                            </div>

                            <div class="table-responsive" style="overflow: auto;">
                                <table class=" mb-0 table mng_act">
                                    <thead>
                                        <tr>
                                            <th>Team Member</th>
                                            <th>Designation</th>
                                            <th>Location</th>
                                            <th>Risk</th>
                                            <th></th>

                                        </tr>
                                    </thead>
                                    <tbody>

                                        <tr id="{{ 'teamRow-' + $index }}" ng-repeat="talent in testTalentList=(talentList | filter: filterCountry)">
                                            <td class="">
                                                <div class="widget-content p-0">
                                                    <a ng-href="/individual-card?data={{hrefDirectFunct(talent)}}">
                                                        <div class="widget-content-wrapper">

                                                            <div class="widget-content-left mr-3">
                                                                <div class="widget-content-left">
                                                                    <img width="40" class="rounded-circle"
                                                                        src="images/placeholder_image.svg" alt="">
                                                                </div>
                                                            </div>
                                                            <div class="widget-content-left flex2">
                                                                <div class="widget-head">{{talent.full_name}}({{talent.email}})</div>

                                                            </div>

                                                        </div>
                                                    </a>
                                                </div>
                                            </td>
                                            <td>{{talent.standard_title}}</td>
                                            <td>{{talent.company_country}}</td>
                                            <td>
                                                <a href="#" type="button" data-toggle="modal"
                                                    data-target="{{(talent.status?'#myModal1':'#myModal2')}}"><img
                                                        ng-src="{{(talent.status?'images/flagred_icon.svg':'images/flagblue_icon.svg')}}"
                                                        ng-click="escalateModelFunct(talent)"></a>
                                                <sub ng-if="talent.status">Risk</sub>
                                                <!-- <a href="#" type="button" data-toggle="modal"
                                                    data-target="#myModal1"><img src="images/flagred_icon.svg"></a>
                                                <sub>Risk</sub> -->
                                            </td>
                                            <td class="text-right">
                                                <div class="dropup" ng-if="talent.status">
                                                    <button class="popclickbtn" id="{{ 'triggerHide-' + $index + '_1'}}"
                                                        onclick="showInfo(this.id);"><img
                                                            src="images/comment_icon.svg"></button>
                                                    <div class="popdiv d-none" id="{{ 'tooltip-' + $index + '_1'}}"
                                                        >
                                                        <div class="tooltip-info">
                                                            <ul class="nav flex-column">
                                                                <li class="nav-item-header nav-item">Comments
                                                                    <img src="images/cross_icon.svg"
                                                                        class="float-right popclosebtn"
                                                                        id="{{ 'triggerShow-' + $index + '_1'}}"
                                                                        onclick="hideInfo(this.id);"></li>
                                                                <li class="nav-item mb-2">
                                                                    <span
                                                                        class="float-left mt-3 ">{{managername}}({{aliasmanager}})</span>
                                                                    <div class="time float-right mt-3">
                                                                        {{talent.create_at}}
                                                                    </div>
                                                                </li>
                                                                <li class="nav-item">
                                                                    <p>{{talent.comment}}
                                                                    </p>
                                                                </li>
                                                            </ul>
                                                        </div>
                                                    </div>
                                                </div>
                                            </td>

                                        </tr>
                                        
                                    </tbody>
                                </table>
                                <div class="col-md-12 col-sm-12 col-xs-12 col text-center" ng-show="!testTalentList.length">
                                    <img  class="rounded-circle"
                                    src="images/No_Talent_Found.svg" alt="">
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
