            var section = $(this).closest(".formSection");
            var otherSections = $(".formSection").not(section);
            var inputs = section.find("input");
            var select = section.find("select");
            var textarea = section.find("textarea");
            var radio = section.find("radio");


            section
                .addClass("readOnly");

            otherSections
                .removeClass("disabled");

            $('button').prop("disabled", false);

            inputs
                .each(function() {
                    $(this).prop("disabled", true)
                })

            select
                .each(function() {
                    $(this).prop("disabled", true)
                })

            textarea
                .each(function() {
                    $(this).prop("disabled", true);
                })

            radio
                .each(function() {
                    $(this).prop("disabled", true)
                })

            // e.stopPropagation();
            // e.preventDefault();
        }).on("click", ".saveButton", function(e) {

            var section = $(this).closest(".formSection");
            var otherSections = $(".formSection").not(section);
            var inputs = section.find("input");
            var select = section.find("select");
            var textarea = section.find("textarea");
            var radio = section.find("radio");


            section
                .addClass("readOnly");
            otherSections
                .removeClass("disabled")
            $('button').prop("disabled", false);
            inputs
                .each(function() {
                    $(this).prop("disabled", true)
                })

            select
                .each(function() {
                    $(this).prop("disabled", true)
                })
            textarea
                .each(function() {
                    $(this).prop("disabled", true)
                })

            radio
                .each(function() {
                    $(this).prop("disabled", true)
                })


            e.stopPropagation();
            e.preventDefault();
