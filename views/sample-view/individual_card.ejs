                var section = $(this).closest(".formSection");
                var otherSections = $(".formSection").not(section);
                var inputs = section.find("input");
                var select = section.find("select");
                var textarea = section.find("textarea");
                var radio = section.find("radio");

                section
                    .removeClass("readOnly");

                otherSections
                    .addClass("disabled")
                    .find('button').prop("disabled", true);

                oldValues = {};
                inputs
                    .each(function() {
                        oldValues[this.id] = $(this).val();
                    })
                    .prop("disabled", false);
                select
                    .each(function() {
                        oldValues[this.id] = $(this).val();
                    })
                    .prop("disabled", false);
                textarea
                    .each(function() {
                        oldValues[this.id] = $(this).val();
                    })
                    .prop("disabled", false);
                radio
                    .each(function() {
                        oldValues[this.id] = $(this).val();
                    })
                    .prop("disabled", false);

            })

