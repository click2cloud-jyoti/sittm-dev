<body class="" id="smmlBody">
    <div ng-app="smmlApp" ng-controller="smmlController" ng-cloak>

        <!-- PAGE CONTAINER 1-START -->
        <div id="notiificationContainer" ng-show="smml_loader">
            <div class="sittmloader">
                <span id="loader_text" style="font-size: 20px;">Fetching Data...!</span>
            </div>
            <!-- <span id="loader_text" style="font-size: 20px;">Fetching Data...!</span> -->
        </div>
        <div class="page-container" ng-show="showContainer" ng-init="init('<%= data %>')">
            <div class="row ">
                <div class="col-md-8">
                    <div class="main-head">
                        <h3 class="card-head m-0">Managers &amp; Team Info</h3>
                    </div>

                </div>
                <div class="col-md-4">
                    <div class="updates float-right text-right">
                        <!-- <p class="mb-0">Last Updated: <span>4/23/2020 4:13 PM</span></p> -->
                        <p class="mb-0">Last Login: <span>{{UserInfo.lastLoginTime || null}}</span></p>
                    </div>
                </div>
            </div>
            <hr>

        </div>
        <!-- PAGE CONTAINER 1-END -->

        <!-- PAGE CONTAINER 2-START -->
        <div class="page-container">
            <!-- END OF ROW MAIN -->
            <div class="row">
                <!--LEFT SECTION COL-MD-4 -->
                <div class="col-md-4 col-sm-4 col-xs-12">
                    <div class="manager-col" style="overflow: auto; outline: none;">
                        <div class="row m-0">
                            <div class="col-md-6 pl_0">
                                <div class="head">
                                    <h3 class="card-head">Managers</h3>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="filter">
                                    <button class="filter-btn float-right">
                                        <p>Filter</p><img src="images/filter_icon.svg" class="float-right">
                                    </button>
                                </div>
                            </div>
                        </div>

                        <ul class="nav nav-tabs rm-list-borders rm-list-borders-scroll list-group list-group-flush"
                            role="tablist">

                            <li class="nav-item list-group-item" ng-repeat="manager in managerInfoList track by $index">
                                <a class="nav-link" ng-class="{'active':$index==0}" href="#profile" role="tab"
                                    data-toggle="tab" ng-click="getManagerInfo(manager)" style="width: 100%;">
                                    <div class="widget-content p-0">
                                        <div class="widget-content-wrapper">
                                            <div class="widget-content-left mr-3">
                                                <img width="42" class="rounded-circle"
                                                    src="images/placeholder_image.svg" alt="" style="margin-top: 1px;">
                                            </div>
                                            <div class="manager_list_left">
                                                <div class="widget-heading">{{manager.name}} ({{manager.email}})</div>
                                                <div class="widget-subheading">{{manager.standard_title}}</div>
                                            </div>
                                        </div>

                                    </div>
                                </a>
                                <div class="widget-content-right manager_profile_eye_left">
                                    <a href="/manager-view?data={{hrefDirectFunct(manager)}}" type="button"
                                        class="btn btn-default custom-tooltip">
                                        <div class="tooltip-text"> View Profile <i class="fa fa-caret-down "
                                                aria-hidden="true"></i></div>
                                    </a>
                                </div>
                            </li>

                        </ul>
                    </div>
                </div>
                <!-- END OF COL-MD-4 -->
                <!-- RIGHT SECTION COL-MD-8 -->
                <div class="col-md-8 col-sm-8 col-xs-12" style="padding-left: 0px;">
                    <!-----------------Dashboard------------------->
                    <div class="row" style="margin-left: 0px;">
                        <div class="col-md-4 col-sm-6 col-xs-12 pl-0 col-pad">
                            <div class="dashboard_smml mb-3 widget-content bg-midnight-bloom">
                                <div class="widget-content-wrapper text-white">
                                    <div class="widget-content-left col-md-10 col-sm-10 col-xs-10">
                                        <div class="db_count">{{totalTalentCount.totaltalent || 0}}</div>
                                        <div class="db_sub_title">Total No. of Talent</div>
                                    </div>
                                    <div class="col-md-2 col-sm-2 col-xs-2">
                                        <div class="widget-numbers text-white">
                                            <span class="count-icons">
                                                <i class="pe-7s-users"></i>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 col-sm-6 col-xs-12 col-pad">
                            <div class="dashboard_smml mb-3 widget-content bg-arielle-smile">
                                <div class="widget-content-wrapper text-white">
                                    <div class="widget-content-left col-md-10 col-sm-10 col-xs-10">
                                        <div class="db_count">{{totalTalentCount.lefttalent || 0}}</div>
                                        <div class="db_sub_title">No. of Talent Left</div>
                                    </div>
                                    <div class="col-md-2 col-sm-2 col-xs-2">
                                        <div class="widget-numbers text-white">
                                            <span class="count-icons">
                                                <i class="pe-7s-less"></i>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4 col-sm-6 col-xs-12 col-pad">
                            <div class="dashboard_smml mb-3 widget-content bg-grow-early">
                                <div class="widget-content-wrapper text-white">
                                    <div class="widget-content-left col-md-10 col-sm-10 col-xs-10">
                                        <div class="db_count">{{totalTalentCount.last30days || 0}}</div>
                                        <div class="db_sub_title">Not Updated Last 30 Days</div>
                                    </div>
                                    <div class="col-md-2 col-sm-2 col-xs-2">
                                        <div class="widget-numbers text-white">
                                            <span class="count-icons">
                                                <i class="pe-7s-user"></i>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- <div class="col-md-6 col-xl-3 col-pad">
                            <div class="card mb-3 widget-content bg-love-kiss">
                                <div class="widget-content-wrapper text-white">
                                    <div class="widget-content-left col-md-10 col-sm-10 col-xs-10">
