    <script>
        function showInfo(id) {
            let x = id.split("-");
            if (x !== null && x.length == 2) {

                // $("#triggerHide-" + x[1]).removeClass("d-inline-block");
                // $("#triggerHide-" + x[1]).addClass("d-none");
                // $("#triggerShow-" + x[1]).removeClass("d-none");
                // $("#triggerShow-" + x[1]).addClass("d-inline-block");
                $("#tooltip-" + x[1]).removeClass("d-none");
                $("#tooltip-" + x[1]).addClass("d-inline-block");
                // $("#tooltip-" + x[1]).attr("tabindex", "1").focus();
            }
        }

        function hideInfo(id) {

            let x = id.split("-");
            if (x !== null && x.length == 2) {
                // $("#triggerHide-" + x[1]).removeClass("d-none");
                // $("#triggerHide-" + x[1]).addClass("d-inline-block");
                // $("#triggerShow-" + x[1]).removeClass("d-inline-block");
                // $("#triggerShow-" + x[1]).addClass("d-none");
                $("#tooltip-" + x[1]).removeClass("d-inline-block");
                $("#tooltip-" + x[1]).addClass("d-none");
                // $("#tooltip-" + x[1]).removeAttr("tabindex");

            }
        }
        $(document).mouseup(function (e) {


            if ($(e.target).closest(".popdiv").length
                === 0) {
                $(".popdiv").removeClass("d-inline-block");
                $(".popdiv").addClass("d-none");
            }
        });

    </script>
    <script>
        $(" #smmlBody").click(function () {
            var x = parent.document.getElementById("userProfileMenu");
            x.classList.remove("show");
            var y = parent.document.getElementById("otherMenu");
            y.classList.remove("show");
            var z = parent.document.getElementById("notificationDropdown");
            z.classList.remove("show");
        });
        $('.nav-link').click(function (e) {
            e.preventDefault();
            $('.nav-link').removeClass('active');
            $(this).addClass('active');
        });
    </script>
