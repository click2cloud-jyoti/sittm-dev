        .dropup .dropdown-menu:before {
            content: '';
            position: absolute;
            display: block;
            width: 0px;
            left: 69%;
            bottom: 5px;
            border: 8px solid transparent;
            border-bottom: 0;
            border-top: 15px solid #fff;
            transform: translate(-50%, calc(100% + 5px));
